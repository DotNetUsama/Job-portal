            <fieldset id="work-experiences-partial" class="sub-form pb24" data-count="0" data-for="WorkExperiences">
                <h2 class="fs-headline2"><i class="fa fa-briefcase"></i> Work experiences</h2>
                <ul class="px28 displayed-objects-list list-unstyled">
                    @foreach (var workExperience in Model.Resume.WorkExperiences)
                    {
                        var cantEdit = workExperience.EndDate.HasValue;
                        <li class="show-item collapse-container fs-body3">
                            <div class="row info-item bg-form-light-main-hover">
                                <div class="col-md-10">
                                    <span>
                                        <span class="font-weight-bold">@workExperience.JobTitle.Title</span>
                                        in
                                        <span class="font-weight-bold">@workExperience.Company.Name</span>
                                    </span>
                                </div>
                                <div class="col-md-2 d-flex">
                                    <button type="button" class="s-btn ml-auto btn-icon extend">
                                        <i class="fa fa-angle-down"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="item-details collapse-content">
                                @if (cantEdit)
                                {
                                    <div class="details-container">
                                        Worked as <span class="font-weight-little-bold">@workExperience.JobTitle.Title</span> in <span class="font-weight-little-bold">@workExperience.Company.Name</span> (from @workExperience.StartDate.Year to @workExperience.EndDate.Value.Year)
                                    </div>
                                    if (!string.IsNullOrEmpty(workExperience.Description))
                                    {
                                        <div class="mt-3">
                                            <span class="d-block font-weight-little-bold">Description:</span>
                                            <div class="mt-2">
                                                @workExperience.Description
                                            </div>
                                        </div>
                                    }
                                }
                                else
                                {
                                    <div class="details-container">
                                        Is working as <span class="font-weight-little-bold">@workExperience.JobTitle.Title</span> in <span class="font-weight-little-bold">@workExperience.Company.Name</span> (started from @workExperience.StartDate.Year)
                                    </div>
                                    if (!string.IsNullOrEmpty(workExperience.Description))
                                    {
                                        <div class="mt-3">
                                            <span class="d-block font-weight-little-bold">Description:</span>
                                            <div class="mt-2">
                                                @workExperience.Description
                                            </div>
                                        </div>
                                    }
                                    <div class="d-flex mt-4 pl-2 pr-2">
                                        <label class="font-weight-bold"
                                               asp-for="WorkExperiencesEdits[workExperiencesEditsIndexer].EndDate"
                                               style="flex: 1; align-self: center"></label>
                                        <input class="form-control"
                                               asp-for="WorkExperiencesEdits[workExperiencesEditsIndexer].EndDate"
                                               style="flex: 3;" />
                                        <input asp-for="WorkExperiencesEdits[workExperiencesEditsIndexer].Id"
                                               value="@workExperience.Id" />
                                    </div>
                                    <span asp-validation-for="WorkExperiencesEdits[workExperiencesEditsIndexer].EndDate"
                                          class="validation-label text-danger"></span>
                                    workExperiencesEditsIndexer++;
                                }
                            </div>
                        </li>
                    }
                </ul>
                <ul class="px28 displayed-objects-list list-unstyled"></ul>
                <div class="hidden-objects-container d-none"></div>
                <div class="px28 pb24 bb bc-black-2">
                    <div class="form-inputs">
                        <div class="input-group">
                            <label asp-for="WorkExperience.JobTitle" class="f-label">Position</label>
                            <input asp-for="WorkExperience.JobTitle" id="work-experience-job-title-input" class="f-input" />
                            <span asp-validation-for="WorkExperience.JobTitle" class="validation-label text-danger fs-body3"></span>
                        </div>
                        <div class="input-group">
                            <label asp-for="WorkExperience.Company" class="f-label">Company or organization</label>
                            <input asp-for="WorkExperience.Company" id="company-input" class="f-input" />
                            <span asp-validation-for="WorkExperience.Company" class="validation-label text-danger fs-body3"></span>
                        </div>
                        <div class="grid gs24 grid__allcells6 apply-form--grid">
                            <div class="grid--cell input-group">
                                <label asp-for="WorkExperience.StartDate" class="f-label">Starting date</label>
                                <input asp-for="WorkExperience.StartDate" class="f-input" />
                                <span asp-validation-for="WorkExperience.StartDate" class="validation-label text-danger fs-body3"></span>
                            </div>
                            <div class="grid--cell input-group">
                                <label asp-for="WorkExperience.EndDate" class="f-label">End date</label>
                                <input asp-for="WorkExperience.EndDate" class="f-input" />
                                <span asp-validation-for="WorkExperience.EndDate" class="validation-label text-danger fs-body3"></span>
                            </div>
                        </div>
                        <div class="input-group">
                            <label asp-for="WorkExperience.Description" class="f-label">Responsibilities</label>
                            <textarea asp-for="WorkExperience.Description" class="f-input"></textarea>
                            <span asp-validation-for="WorkExperience.Description" class="validation-label text-danger fs-body3"></span>
                        </div>
                    </div>
                    <div class="form-actions">
                        <button type="button" class="s-btn btn-clear btn-sub-submit">
                            <i class="fa fa-plus-circle"></i> Add
                        </button>
                    </div>
                </div>
            </fieldset>

            <fieldset id="educations-partial" class="sub-form tab pb24" data-count="0" data-for="Educations">
                <h2 class="fs-headline2"><i class="fa fa-graduation-cap"></i> Educations</h2>
                <ul class="px28 displayed-objects-list list-unstyled">
                    @foreach (var education in Model.Resume.Educations)
                    {
                        var cantEdit = education.EndDate.HasValue;
                        <li class="show-item collapse-container fs-body3">
                            <div class="row info-item bg-form-light-main-hover">
                                <div class="col-md-10">
                                    <span>
                                        <span class="font-weight-bold">@education.FieldOfStudy.Title</span>
                                        in
                                        <span class="font-weight-bold">@education.School.Name</span>
                                    </span>
                                </div>
                                <div class="col-md-2 d-flex">
                                    <button type="button" class="s-btn ml-auto btn-icon extend">
                                        <i class="fa fa-angle-down"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="item-details collapse-content">
                                @if (cantEdit)
                                {
                                    <div class="details-container">
                                        Graduated from <span class="font-weight-little-bold">@education.School.Name</span> with <span class="font-weight-little-bold">@((EducationDegree)education.Degree)</span> degree in <span class="font-weight-little-bold">@education.FieldOfStudy.Title</span> (from @education.StartDate.Year to @education.EndDate.Value.Year)
                                    </div>
                                }
                                else
                                {
                                    <div class="details-container">
                                        Is studying <span class="font-weight-little-bold">@education.FieldOfStudy.Title</span> in <span class="font-weight-little-bold">@education.School.Name</span> to get <span class="font-weight-little-bold">@((EducationDegree)education.Degree)</span> (started from @education.StartDate.Year)
                                    </div>
                                    <div class="d-flex mt-4 pl-2 pr-2">
                                        <label class="font-weight-bold"
                                               asp-for="EducationsEdits[educationsEditsIndexer].EndDate"
                                               style="flex: 1; align-self: center"></label>
                                        <input class="form-control"
                                               asp-for="EducationsEdits[educationsEditsIndexer].EndDate"
                                               style="flex: 3;" />
                                        <input asp-for="EducationsEdits[educationsEditsIndexer].Id"
                                               value="@education.Id" />
                                    </div>
                                    <span asp-validation-for="EducationsEdits[educationsEditsIndexer].EndDate"
                                          class="validation-label text-danger"></span>
                                    educationsEditsIndexer++;
                                }
                            </div>
                        </li>
                    }
                </ul>
                <div class="hidden-objects-container d-none"></div>
                <div class="px28 pb24 bb bc-black-2">
                    <div class="form-inputs">
                        <div class="input-group">
                            <label asp-for="Education.School" class="f-label">University or institution</label>
                            <input asp-for="Education.School" id="school-input" class="f-input" />
                            <span asp-validation-for="Education.School" class="validation-label text-danger fs-body3"></span>
                        </div>
                        <div class="grid gs24 grid__allcells6 apply-form--grid">
                            <div class="grid--cell input-group">
                                <label asp-for="Education.FieldOfStudy" class="f-label"></label>
                                <input asp-for="Education.FieldOfStudy"
                                       id="field-of-study-input" class="f-input" />
                                <span asp-validation-for="Education.FieldOfStudy" class="validation-label text-danger fs-body3"></span>
                            </div>
                            <div class="grid--cell input-group">
                                <label asp-for="Education.Degree" class="f-label"></label>
                                <select asp-for="Education.Degree"
                                        asp-items="Html.GetEnumSelectList<EducationDegree>()"
                                        class="f-input">
                                    <option value="">Please select</option>
                                </select>
                                <span asp-validation-for="Education.Degree" class="validation-label text-danger fs-body3"></span>
                            </div>
                        </div>
                        <div class="grid gs24 grid__allcells6 apply-form--grid">
                            <div class="grid--cell input-group">
                                <label asp-for="Education.StartDate" class="f-label">Starting date</label>
                                <input asp-for="Education.StartDate" class="f-input" />
                                <span asp-validation-for="Education.StartDate" class="validation-label text-danger fs-body3"></span>
                            </div>
                            <div class="grid--cell input-group">
                                <label asp-for="Education.EndDate" class="f-label">Graduating date</label>
                                <input asp-for="Education.EndDate" class="f-input" />
                                <span asp-validation-for="Education.EndDate" class="validation-label text-danger fs-body3"></span>
                            </div>
                        </div>
                    </div>
                    <div class="form-actions">
                        <button type="button" class="s-btn btn-clear btn-sub-submit">
                            <i class="fa fa-plus-circle"></i> Add
                        </button>
                    </div>
                </div>
            </fieldset>

