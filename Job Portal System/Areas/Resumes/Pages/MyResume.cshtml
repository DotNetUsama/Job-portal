@page
@using Enums
@model MyResumeModel
@{
    ViewData["Title"] = "Your Resume";
    var educationsEditsIndexer = 0;
    var workExperiencesEditsIndexer = 0;
    var skillsEditsIndexer = 0;
}

<div class="wmx6 m-auto ba bc-black-2 my24">
    <div class="px28 py24 ps-relative">
        <div class="bb bc-black-2">
            <h1 class="fs-headline1 ta-center mb32 mt12">@ViewData["Title"]</h1>
        </div>
        <form method="post" class="pt24">
            <fieldset id="general-info-partial" class="pb24">
                <h2 class="fs-headline2"><i class="fa fa-question-circle"></i> General information</h2>
                <div class="px28 pb24 bb bc-black-2">
                    <div class="input-group">
                        <label asp-for="ResumeInfo.Biography" class="f-label">Biography</label>
                        <textarea asp-for="ResumeInfo.Biography" class="f-input">
                                @Model.ResumeInfo.Biography
                        </textarea>
                        <span asp-validation-for="ResumeInfo.Biography" class="validation-label text-danger fs-body3"></span>
                    </div>
                    <div class="grid gs24 grid__allcells6 apply-form--grid">
                        <div class="grid--cell input-group">
                            <label asp-for="ResumeInfo.MinSalary" class="f-label">Minimum salary</label>
                            <input asp-for="ResumeInfo.MinSalary" class="f-input" />
                            <span asp-validation-for="ResumeInfo.MinSalary" class="validation-label text-danger fs-body3"></span>
                        </div>
                        <div class="grid--cell input-group">
                            <label asp-for="ResumeInfo.MovingDistanceLimit" class="f-label">Moving distance limit</label>
                            <input asp-for="ResumeInfo.MovingDistanceLimit" class="f-input" />
                            <span asp-validation-for="ResumeInfo.MovingDistanceLimit" class="validation-label text-danger fs-body3"></span>
                        </div>
                    </div>
                    <label title="Your resume will be recommended to job vacancies and you'll be notified" class="d-block fs-body2">
                        <input asp-for="ResumeInfo.IsSeeking">
                        Mark this if you're seeking for a job
                    </label>
                    <label title="Your resume will be visible to anyone in public" class="d-block fs-body2">
                        <input asp-for="ResumeInfo.IsPublic">
                        Make my resume public
                    </label>

                    <div class="input-group">
                        <label class="f-label">Preferred job types</label>
                        <div class="grid fw-wrap">
                            @foreach (var (key, _) in Model.ResumeInfo.JobTypes)
                            {
                                <label title="Your resume will be visible to anyone in public" class="fl-third fs-body2">
                                    <input asp-for="ResumeInfo.JobTypes[key]">
                                    @key.GetDescription()
                                </label>
                            }
                        </div>
                    </div>
                </div>
            </fieldset>
